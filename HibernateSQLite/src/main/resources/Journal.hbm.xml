<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.Journal" table="journal">
		<id name="id" column="id" type="long">
			<generator class="native"></generator>
		</id>
		<property name="dateTime" column="date_time" type="string"></property>
		<property name="time" column="time" type="string"></property>
		<property name="user" column="user" type="string"></property>
		<property name="action" column="action" type="string"></property>
		
	</class>
</hibernate-mapping>